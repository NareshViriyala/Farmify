<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nareshviriyala.farmifyagentfarmer">

    <uses-permission android:name="android.permission.INTERNET"/>
    <permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Farmify"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/MyMaterialTheme">
        <activity android:name=".Activities.BootActivity">

        </activity>

        <activity android:name=".Activities.RegisterActivity" android:windowSoftInputMode="stateHidden|adjustResize">

        </activity>

        <activity android:name=".Activities.MainActivity" android:windowSoftInputMode="stateHidden|adjustResize">

        </activity>

        <activity android:name=".Activities.HomeActivity" android:windowSoftInputMode="stateHidden|adjustResize">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
    </application>

</manifest>